{
  "name": "ueno-next-starter",
  "description": "Opinionated Next starter by Ueno",
  "version": "2.0.0",
  "license": "MIT",
  "keywords": [
    "next",
    "starter",
    "react",
    "typescript",
    "next-starter",
    "nextjs",
    "ueno"
  ],
  "contributors": [
    "Birkir Gudjonsson <birkir.gudjonsson@gmail.com>",
    "Jérémy Barbet <jeremgraph@gmail.com>"
  ],
  "scripts": {
    "dev": "ts-node-dev --transpileOnly --project src/tsconfig.json --compiler-options '{\"module\":\"commonjs\"}' --no-notify --ignore-watch .next src/server",
    "build": "next build src && tsc --project src/tsconfig.json",
    "start": "cross-env NODE_ENV=production next start build",
    "export": "next export -o ./dist src",
    "lint": "npm run tslint && npm run stylelint",
    "tslint": "tslint --fix 'src/**/*.{ts,tsx}' -p .",
    "stylelint": "stylelint 'src/**/*.{ts,tsx}'",
    "prettier": "prettier --list-different '**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "tslint --fix"
    ]
  },
  "stylelint": {
    "processors": [
      "stylelint-processor-styled-components"
    ],
    "extends": [
      "stylelint-config-recommended",
      "stylelint-config-styled-components"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  },
  "dependencies": {
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.15",
    "next": "9.3.4",
    "next-compose-plugins": "2.2.0",
    "next-fonts": "1.0.3",
    "next-images": "1.4.0",
    "next-react-svg": "1.1.0",
    "next-videos": "1.1.4",
    "polished": "3.5.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "5.2.1",
    "styled-components": "5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@types/express": "4.17.4",
    "@types/lodash": "4.14.149",
    "@types/next": "9.0.0",
    "@types/node": "13.9.8",
    "@types/react": "16.9.31",
    "@types/react-dom": "16.9.6",
    "@types/react-helmet": "5.0.15",
    "@types/styled-components": "5.0.1",
    "babel-plugin-styled-components": "1.10.7",
    "cross-env": "7.0.2",
    "husky": "4.2.3",
    "lint-staged": "10.1.1",
    "prettier": "2.0.2",
    "stylelint": "13.2.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "svg-react-loader": "0.4.6",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "6.1.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.2.0",
    "typescript": "3.8.3"
  }
}
